{
  "name": "mp-runtime",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node --loader ts-node/esm src/main.ts",
    "check": "tsc --noEmit --project tsconfig.json",
    "migrate": "node --loader ts-node/esm src/db/migrate.ts",
    "demo": "node --loader ts-node/esm src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-fastify": "^11.1.8",
    "fastify": "^5.6.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.8",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.10.6",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^4.0.5"
  }
}
